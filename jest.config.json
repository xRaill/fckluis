{
  "setupFilesAfterEnv": ["./jest.setup.js"],
  "moduleNameMapper": {
    "^tests(.*)$": "<rootDir>/tests$1",
    "^pages(.*)$": "<rootDir>/pages$1",
    "^components(.*)$": "<rootDir>/components$1",
    "^hooks(.*)$": "<rootDir>/hooks$1",
    "^utils(.*)$": "<rootDir>/utils$1"
  },
  "collectCoverage": true,
  "collectCoverageFrom": [
    "./pages/**",
    "!./pages/_*",
    "./components/**",
    "./hooks/**",
    "./utils/**"
  ],
  "coverageThreshold": {
    "global": {
      "lines": 100
    }
  }
}
